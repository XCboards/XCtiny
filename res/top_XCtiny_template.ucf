# -- [ Clock inputs ] ---------------------------------------------------------
# "CLK0" - on-board 8MHz oscillator
#NET "CLK0"      LOC="P6"  | IOSTANDARD=LVCMOS33 | BUFG=CLK  | TNM_NET = CLK0;
#TIMESPEC TS_CLK0 = PERIOD "CLK0" 8.0000 MHz HIGH 50%;
# "CLK1" - on-board 7.8125kHz oscillator
#NET "CLK1"      LOC="P7"  | IOSTANDARD=LVCMOS33 | BUFG=CLK  | TNM_NET = CLK1;
#TIMESPEC TS_CLK1 = PERIOD "CLK1" 7.8125 KHz HIGH 50%;

# -- [ Global reset ] ---------------------------------------------------------
# "nGSR" - on-board reset button. Active-low!
#NET "nGSR"      LOC="P31" | IOSTANDARD=LVCMOS33 | BUFG=SR   | SCHMITT_TRIGGER;
#NET "nGSR"      LOC="P31" | IOSTANDARD=LVCMOS18 | BUFG=SR   | SCHMITT_TRIGGER;

# -- [ Input-only signals ] ---------------------------------------------------
# "GPI" - general purpose input (pin 22)
#NET "GPI"       LOC="P22" | IOSTANDARD=LVCMOS33;
#NET "GPI"       LOC="P22" | IOSTANDARD=LVCMOS18;

# -- [ GPIO ] -----------------------------------------------------------------
