#NET "GPIO<10>"  LOC="P19" | IOSTANDARD=LVCMOS33;
#NET "GPIO<11>"  LOC="P23" | IOSTANDARD=LVCMOS33;
#NET "GPIO<12>"  LOC="P24" | IOSTANDARD=LVCMOS33;
